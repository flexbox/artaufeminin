{
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "slug",
          "placeholder": "Url de la critique du livre"
        }
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "single": "heading1",
          "label": "title",
          "placeholder": "titre du livre"
        }
      },
      "content": {
        "type": "StructuredText",
        "config": {
          "multi": "paragraph, heading2, heading3, heading4, strong, em, hyperlink, image, embed, list-item, o-list-item",
          "allowTargetBlank": true,
          "label": "content",
          "placeholder": "Critique / avis sur le livre"
        }
      }
    }
  }